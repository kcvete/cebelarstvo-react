# Stripe Configuration
# Get your keys from https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Backend Stripe Secret Key (DO NOT commit this to version control)
# This should be used in your backend only, never exposed to the frontend
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Cloudflare Worker URL for Stripe Checkout
# Replace with your actual worker URL from Cloudflare dashboard
VITE_CLOUDFLARE_WORKER_URL=https://honey-shop-backend.YOUR_SUBDOMAIN.workers.dev

# Gemini API Key
VITE_GEMINI_API_KEY=your_gemini_api_key_here
